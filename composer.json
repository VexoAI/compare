{
    "name": "vexo/compare",
    "description": "A collection of functions to compare vectors",
    "keywords": ["vexo", "vector", "compare"],
    "license": "MIT",
    "homepage": "https://vexo.ai/",
    "authors": [
        {
            "name": "Frank van den Brink",
            "email": "frank@pragmatist.nl"
        }
    ],
    "require": {
        "php": ">=8.1"
    },
    "autoload": {
        "files": [
            "src/Distance.php",
            "src/Similarity.php"
        ]
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "^3.17",
        "phpstan/phpstan": "^1.10",
        "phpunit/php-code-coverage": "^10.1",
        "phpunit/phpunit": "^10.1",
        "rector/rector": "^0.16.0",
        "symfony/var-dumper": "^6.2",
        "pestphp/pest": "^2.6"
    },
    "config": {
        "allow-plugins": {
            "pestphp/pest-plugin": true
        },
        "sort-packages": true
    }
}
